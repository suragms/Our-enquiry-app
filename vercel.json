{
    "buildCommand": "prisma generate && npm run build",
    "installCommand": "npm install",
    "framework": "vite",
    "functions": {
        "api/index.ts": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}